}


/* 定位 */
.re {
  position: relative
}

.re1 {
  position: absolute
}

.dis {
  display: inline-block;
  max-width: 500rpx;
}

.center {
  text-align: center
}

.pa {
  padding: 0 40rpx
}

.pa1 {
  padding: 10pt 20pt
}

.clearfix {
  clear: both
}

.right {
  float: right
}

.left {
  float: left
}

.f_w {
  font-weight: bold
}

/*自定义底部导航开始  */
.tabbar_box {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 110rpx;
  border-top: 1rpx solid #f0f0f0;
}

.tabbar_nav {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  height: 100%;
}

.tabbar_icon {
  width: 56rpx;
  height: 56rpx;
}

/*底部导航结束 */

.line {
  line-height: 2
}

.line1 {
  line-height: 1.4
}

.line2 {
  line-height: 1.6
}

.phcolor {
  color: #B2B2B2;
}

swiper {
  height: 180px
}

/* 弹出 */
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 90000000;
  opacity: 0.4;
}

.modalDlg {
  width: 76%;
  background: #fff;
  position: fixed;
  top: 34%;
  left: 12%;
  right: 12%;
  z-index: 90000009;
  display: flex;
  flex-direction: column;
  align-items: center;
}


