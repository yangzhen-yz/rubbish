/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

/*全局css  */
page {
  background-color: #F5F5F5;
}

/* 颜色 */
.color {
  color: #181818
}

.color1 {
