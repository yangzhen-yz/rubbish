  font-size: 20rpx;
}

.f11 {
  font-size: 22rpx;
}

.f12 {
  font-size: 24rpx
}

.f13 {
  font-size: 26rpx;
}

.f14 {
  font-size: 28rpx
}

.f15 {
  font-size: 30rpx;
}

.f16 {
  font-size: 32rpx
}

.f17 {
  font-size: 34rpx;
}

.f18 {
  font-size: 36rpx
}

.f19 {
  font-size: 38rpx;
}

.f20 {
