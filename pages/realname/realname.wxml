<view class='pa1'>
  <block wx:if="{{memdata.is_ren==0}}">
    <form bindsubmit="formSubmit">
      <view class='line f16'>
        <view class='dis'>姓氏：</view>
        <input id="member_name" name="member_name" placeholder-class="phcolor" class="f16 dis bor" type="text"
          placeholder="" style='width: 78%;vertical-align:middle;padding:4px 10px;' />
      </view>
      <view class='line f16'>
        <view class='dis'>性别：</view>
        <radio-group class="radio-group dis center" bindchange="radioChange">
          <label>
            <radio name="sex" value="0" checked="" disabled="{{item.disabled}}" /><text space="ensp"> 男 </text>
          </label>
          <label>
            <radio name="sex" value="1" checked="" disabled="{{item.disabled}}" /><text space="ensp"> 女 </text>
          </label>
        </radio-group>

        <text>{{seleted}}</text>
      </view>
      <view class='line f16'>
        <view class='dis'>年龄：</view>
        <input id="age" name="age" placeholder-class="phcolor" class="f16 dis bor" type="text" placeholder=""
          style='width: 78%;vertical-align:middle;;padding:4px 10px' />
      </view>
      <view class='line f16'>
        <view class='dis'>小区：</view>
        <picker style='width:78%;padding:2px 10px' class='dis bor' name='addressid' bindchange="bindPickerChange"
          value="{{objectIndex}}" range="{{objectArray}}" range-key="home">
          <view>
            {{objectArray[objectIndex].home}}
          </view>
        </picker>
      </view>
      <view class='line f16' wx:if="{{showModal}}">
        <view class='dis'>小区名称：</view>
        <input id="address" name="address" placeholder-class="phcolor" class="f16 dis bor" type="text" placeholder=""
          style='width: 68%;vertical-align:middle;;padding:4px 10px' />
      </view>
      <view class='line f16'>
        <view class='dis'>住址：</view>
        <input id="floor1" name="floor1" class="f16 dis bor center" type="number"
          style='width: 12%;vertical-align:middle;;padding:1px 2px' />号楼
        <input id="floor2" name="floor2" class="f16 dis bor center" type="number"
          style='width: 12%;vertical-align:middle;;padding:1px 2px' />单元
        <input id="floor3" name="floor3" class="f16 dis bor center" type="number"
          style='width: 16%;vertical-align:middle;;padding:1px 2px' />门牌号
      </view>
      <view class='center' style='margin-top:30pt'>
        <button class="btn_our mar f16" wx:if="{{!disabled}}" formType="submit" bindsubmit="formSubmit"
          style='width: 84%;background:-webkit-linear-gradient(left top, #57D949, #2BBC60);color:#fff;line-height:2.8;'>确认提交</button>
        <button class="btn_our mar f16" wx:if="{{disabled}}"
          style='width: 84%;background:-webkit-linear-gradient(left top, #57D949, #2BBC60);color:#fff;line-height:2.8;'>确认提交</button>
      </view>
    </form>
  </block>
  <block wx:if="{{memdata.is_ren==1}}">
    <view class='line f16'>
      <view class='dis'>姓氏：</view>
      <input id="member_name" name="member_name" value="{{memdata.member_name}}" disabled='true'
        placeholder-class="phcolor" class="f16 dis bor" type="text" placeholder=""
        style='width: 78%;vertical-align:middle;padding:4px 10px;' />
    </view>
    <view class='line f16'>
      <view class='dis'>性别：</view>
      <radio-group class="radio-group dis center" bindchange="radioChange">
        <block wx:if="{{memdata.sex==0}}">
          <label>
            <radio name="sex" value="0" checked="checked" disabled="{{item.disabled}}" /><text space="ensp"> 男 </text>
          </label>
          <label>
            <radio name="sex" value="1" checked="" disabled="{{item.disabled}}" /><text space="ensp"> 女 </text>
          </label>
        </block>
        <block wx:if="{{memdata.sex==1}}">
          <label>
            <radio name="sex" value="0" checked="" disabled="{{item.disabled}}" /><text space="ensp"> 男 </text>
          </label>
          <label>
            <radio name="sex" value="1" checked="checked" disabled="{{item.disabled}}" /><text space="ensp"> 女 </text>
          </label>
        </block>
      </radio-group>
      <text>{{seleted}}</text>
    </view>
    <view class='line f16'>
      <view class='dis'>年龄：</view>
      <input id="age" name="age" value="{{memdata.age}}" disabled='true' placeholder-class="phcolor" class="f16 dis bor"
        type="text" placeholder="" style='width: 78%;vertical-align:middle;;padding:4px 10px' />
    </view>
    <view class='line f16'>
      <view class='dis'>小区：</view>
      <input id="xiaoqu" name="xiaoqu" value="{{memdata.xiaoqu}}" disabled='true' class="f16 dis bor" type="text"
        style='width: 78%;vertical-align:middle;;padding:4px 10px' />
    </view>
    <view class='line f16' wx:if="{{memdata.xiaoqu == '其他'}}">
      <view class='dis'>小区名称：</view>
      <input id="address" name="address" value="{{memdata.address}}" disabled='true' class="f16 dis bor" type="text"
        placeholder="" style='width: 68%;vertical-align:middle;;padding:4px 10px' />
    </view>
    <view class='line f16'>
      <view class='dis'>住址：</view>
      <input id="floor" name="floor" value="{{memdata.floor}}" disabled='true' class="f16 dis bor" type="text"
        style='width: 78%;vertical-align:middle;;padding:4px 10px' />
    </view>
    <view class='center' style='margin-top:30pt'>
      <button class="btn_our mar f16"
        style='width: 84%;background:-webkit-linear-gradient(left top, #57D949, #2BBC60);color:#fff;line-height:2.8;'>已完善</button>
    </view>
  </block>
</view>