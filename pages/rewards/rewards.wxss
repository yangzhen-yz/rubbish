page {
  background: #F5F5F5
}

.tab-h {
  height: 87rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.tab-item {
  color: #fff;
  height: 100%;
  display: flex;
  align-items: center;
}

.tab-item.active {
  border-bottom: 4px solid #fff;
  font-weight: bold;
}

.item_view {
  background-color: #FFFFFF;
  border-radius: 20rpx;
  margin: 20rpx;
}

.top_view {
  padding-top: 20rpx;
  display: flex;
  justify-content: space-between;
  margin: 0 26rpx;
  padding-bottom: 20rpx;
  border-bottom: solid 1rpx #F5F5F5;
}

.bottom_view {
  line-height: 41rpx;
  margin: 18rpx 26rpx;
  padding-bottom: 31rpx;
}

.scrollView{
  height: 1300rpx;
}